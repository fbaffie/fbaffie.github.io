---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>fbaffie</title>
 <link href="http://fbaffie.github.io" rel="self"/>
 <link href="http://fbaffie.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://fbaffie.github.io/</id>
 <author>
   <name>fbaffie</name>
   <email>to.come@email.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://fbaffie.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://fbaffie.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>